{
    "name": "cxappserver",
    "deploy": "/public",
    "welcome": "cxserverapp online. all systems green.",
    "apps": [
        {
            "name": "fun",
            "paths": [
                {
                    "route": "/*",
                    "public": "/",
                    "html": "/index.html",
                    "js": [
                        {
                            "path": "js/main.js",
                            "src": "/source/apps/fun/main.coffee"
                        }
                    ],
                    "css": [
                        {
                            "src": "/source/apps/fun"
                        }
                    ]
                }
            ]
        }
    ]
}